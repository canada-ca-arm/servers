{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "objectect": {
            "type": "object",
            "metadata": {
                "description": "Object of MS firewall objects."
            }
        },
        "containerSasToken": {
            "type": "string"
        },
        "_artifactsLocation": {
            "type": "string",
            "metadata": {
                "description": "The base URI where artifacts required by this template are located including a trailing '/'"
            },
            "defaultValue": "[deployment().properties.templateLink.uri]"
        }
    },
    "variables": {
        "backupItemName": "[concat('vm;iaasvmcontainerv2;', resourceGroup().name,';',parameters('objectect').vm.computerName)]",
        "domainJoinURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/domainjoin.json'), parameters('containerSasToken'))]",
        "linuxDomainJoinURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/linuxdomainjoin.json'), parameters('containerSasToken'))]",
        "virtualMachineURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/virtualmachine.json'), parameters('containerSasToken'))]",
        "encryptDisksLinuxURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/encryptVMDisksLinux.json'), parameters('containerSasToken'))]",
        "encryptDisksWindowsURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/encryptVMDisksWindows.json'), parameters('containerSasToken'))]",
        "omsAgentForLinuxURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/omsAgentForLinux.json'), parameters('containerSasToken'))]",
        "omsAgentForWindowsURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/omsAgentForWindows.json'), parameters('containerSasToken'))]",
        "windowsAntimalwareURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/windowsAntimalware.json'), parameters('containerSasToken'))]",
        "publicIPAddressURI": "[concat(uri(parameters('_artifactsLocation'), 'nested/publicIP.json'), parameters('containerSasToken'))]",
        "iisExtensionName": "iisExtension",
        "iisConfigurationFunction": "PrepareWebServer.ps1\\PrepareWebServer",
        "iisConfigurationScript": "PrepareWebServer.ps1.zip",
        "iisForceUpdateTag": "1.0",
        "iisModuleUrl": "[concat(uri(parameters('_artifactsLocation'), concat('DSC','/', variables('iisConfigurationScript'))), parameters('containerSasToken'))]"
    },
    "functions": [
        {
            "namespace": "pspc",
            "members": {
                "arrayASGConfig": {
                    "parameters": [
                        {
                            "name": "ASGArray",
                            "type": "array"
                        }
                    ],
                    "output": {
                        "type": "array",
                        "value": "[parameters('ASGArray')]"
                    }
                },
                "getResourceGroupID": {
                    "parameters": [
                        {
                            "name": "resourceGroupName",
                            "type": "string"
                        }
                    ],
                    "output": {
                        "type": "string",
                        "value": "[uniqueString(concat('/subscriptions/', subscription().subscriptionId, '/resourceGroups/', parameters('resourceGroupName')))]"
                    }
                },
                "getUniqueKeyvaultName": {
                    "parameters": [
                        {
                            "name": "keyVaultName",
                            "type": "string"
                        },
                        {
                            "name": "resourceGroupID",
                            "type": "string"
                        }
                    ],
                    "output": {
                        "type": "string",
                        "value": "[if(contains(parameters('keyvaultName'), '[unique]'), replace(parameters('keyvaultName'), '[unique]', parameters('resourceGroupID')), parameters('keyvaultName'))]"
                    }
                },
                "getValidKeyvaultName": {
                    "parameters": [
                        {
                            "name": "keyvaultName",
                            "type": "string"
                        }
                    ],
                    "output": {
                        "type": "string",
                        "value": "[if(greater(length(parameters('keyvaultName')),24), concat(substring(parameters('keyvaultName'), 0, 24)), parameters('keyvaultName'))]"
                    }
                },
                "getPublicIPAddressResource": {
                    "parameters": [
                        {
                            "name": "computerName",
                            "type": "string"
                        },
                        {
                            "name": "networkInterfaceName",
                            "type": "string"
                        }
                    ],
                    "output": {
                        "type": "object",
                        "value": {
                            "id": "[resourceId ('Microsoft.Network/publicIPAddresses/', concat(parameters('computerName'), '-', parameters('networkInterfaceName'), '-pubIP'))]"
                        }
                    }
                },
                "replaceInObject": {
                    "parameters": [
                        {
                            "name": "object",
                            "type": "object"
                        },
                        {
                            "name": "search",
                            "type": "string"
                        },
                        {
                            "name": "replace",
                            "type": "string"
                        }
                    ],
                    "output": {
                        "type": "object",
                        "value": "[json(replace(string(parameters('object')), parameters('search'), parameters('replace')))]"
                    }
                }
            }
        }
    ],
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "name": "[concat('Deploy-', parameters('objectect').vm.computerName)]",
            "apiVersion": "2018-02-01",
            "dependsOn": [
                "[concat('Microsoft.Network/networkInterfaces/', parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name)]",
                "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name, '-makeIPStatic')]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('virtualMachineURI')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "adminPassword": {
                        "reference": {
                            "keyVault": {
                                "id": "[concat('/subscriptions/', subscription().subscriptionId, '/resourceGroups/', parameters('objectect').vmKeyVault.keyVaultResourceGroupName, '/providers/Microsoft.KeyVault/vaults/', pspc.getValidKeyvaultName(pspc.getUniqueKeyvaultName(parameters('objectect').vmKeyVault.keyVaultName, pspc.getResourceGroupID(parameters('objectect').vmKeyVault.keyVaultResourceGroupName))))]"
                            },
                            "secretName": "[parameters('objectect').vm.adminPassword]"
                        }
                    },
                    "objectect": {
                        "value": "[parameters('objectect')]"
                    },
                    "keyVaultId": {
                        "value": "[concat('/subscriptions/', subscription().subscriptionId, '/resourceGroups/', parameters('objectect').vmKeyVault.keyVaultResourceGroupName, '/providers/Microsoft.KeyVault/vaults/', pspc.getValidKeyvaultName(pspc.getUniqueKeyvaultName(parameters('objectect').vmKeyVault.keyVaultName, pspc.getResourceGroupID(parameters('objectect').vmKeyVault.keyVaultResourceGroupName))))]"
                    },
                }
            }
        },
        {
            "condition": "[contains(parameters('objectect').vm,'shutdownConfig')]",
            "name": "[concat('shutdown-computevm-', parameters('objectect').vm.computerName)]",
            "type": "Microsoft.DevTestLab/schedules",
            "apiVersion": "2017-04-26-preview",
            "tags": "[parameters('objectect').tagValues]",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "status": "[parameters('objectect').vm.shutdownConfig.autoShutdownStatus]",
                "taskType": "ComputeVmShutdownTask",
                "dailyRecurrence": {
                    "time": "[parameters('objectect').vm.shutdownConfig.autoShutdownTime]"
                },
                "timeZoneId": "[parameters('objectect').vm.shutdownConfig.autoShutdownTimeZone]",
                "targetResourceId": "[resourceId('Microsoft.Compute/virtualMachines', parameters('objectect').vm.computerName)]",
                "notificationSettings": {
                    "status": "[parameters('objectect').vm.shutdownConfig.autoShutdownNotificationStatus]",
                    "timeInMinutes": "30"
                }
            }
        },
        {
            "condition": "[contains(parameters('objectect').networkInterfaces,'publicIPAddressesProperties')]",
            "name": "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name, '-pubIP')]",
            "apiVersion": "2015-01-01",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('publicIPAddressURI')]"
                },
                "parameters": {
                    "publicIPAddressProperties": {
                        "value": "[parameters('objectect').networkInterfaces.publicIPAddressesProperties]"
                    },
                    "publicIPAddressName": {
                        "value": "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name, '-pubIP')]"
                    },
                    "tags": {
                        "value": "[parameters('objectect').tagValues]"
                    }
                }
            }
        },
        {
            "name": "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name)]",
            "type": "Microsoft.Network/networkInterfaces",
            "apiVersion": "2019-02-01",
            "tags": "[parameters('objectect').tagValues]",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[concat('Microsoft.Network/networkSecurityGroups/', parameters('objectect').vm.computerName, '-', parameters('objectect').networkSecurityGroups.name)]",
                "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name, '-pubIP')]"
            ],
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipconfig1",
                        "properties": {
                            "privateIPAddress": "[if(contains(parameters('objectect').networkInterfaces, 'privateIPAddress'), parameters('objectect').networkInterfaces.privateIPAddress, json('null'))]",
                            "privateIPAllocationMethod": "[if(contains(parameters('objectect').networkInterfaces, 'privateIPAddress'), 'Static', 'Dynamic')]",
                            "subnet": {
                                "id": "[concat(subscription().id, '/resourceGroups/', parameters('objectect').networkInterfaces.vnetResourceGroupName, '/providers/Microsoft.Network/virtualNetworks/', parameters('objectect').networkInterfaces.vnetName, '/subnets/', parameters('objectect').networkInterfaces.subnetName)]"
                            },
                            "publicIPAddress": "[if(contains(parameters('objectect').networkInterfaces, 'publicIPAddressesProperties'), pspc.getPublicIPAddressResource(parameters('objectect').vm.computerName, parameters('objectect').networkInterfaces.name), json('null'))]",
                            "applicationSecurityGroups": "[if(contains(parameters('objectect').vm, 'applicationSecurityGroups'), pspc.arrayASGConfig(parameters('objectect').vm.applicationSecurityGroups), json('null'))]"
                        }
                    }
                ],
                "dnsSettings": {
                    "dnsServers": "[if(contains(parameters('objectect').networkInterfaces, 'dnsServerPrivateIp'), parameters('objectect').networkInterfaces.dnsServerPrivateIp, json('[]'))]"
                },
                "enableAcceleratedNetworking": "[parameters('objectect').networkInterfaces.acceleratedNetworking]",
                "networkSecurityGroup": {
                    "id": "[resourceId('Microsoft.Network/networkSecurityGroups', concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkSecurityGroups.name))]"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "name": "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name, '-makeIPStatic')]",
            "apiVersion": "2017-08-01",
            "dependsOn": [
                "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name)]",
                "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name, '-pubIP')]"
            ],
            "condition": "[not(contains(parameters('objectect').networkInterfaces, 'privateIPAddress'))]",
            "properties": {
                "mode": "Incremental",
                "template": {
                    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
                    "contentVersion": "1.0.0.0",
                    "parameters": {},
                    "variables": {},
                    "resources": [
                        {
                            "type": "Microsoft.Network/networkInterfaces",
                            "name": "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name)]",
                            "apiVersion": "2018-03-01",
                            "location": "[resourceGroup().location]",
                            "properties": "[pspc.replaceInObject(reference(concat('Microsoft.Network/networkInterfaces/', concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name))), '\"privateIPAllocationMethod\": \"Dynamic\"', '\"privateIPAllocationMethod\": \"Static\"')]"
                        }
                    ]
                }
            }
        },
        {
            "name": "[concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkSecurityGroups.name)]",
            "type": "Microsoft.Network/networkSecurityGroups",
            "apiVersion": "2018-01-01",
            "tags": "[parameters('objectect').tagValues]",
            "location": "[resourceGroup().location]",
            "properties": "[parameters('objectect').networkSecurityGroups.properties]"
        },
        {
            "condition": "[contains(parameters('objectect').vm,'backupConfig')]",
            "apiVersion": "2017-05-10",
            "name": "[concat(parameters('objectect').vm.computerName, '-' , 'BackupIntent')]",
            "type": "Microsoft.Resources/deployments",
            "resourceGroup": "[if(contains(parameters('objectect').vm,'backupConfig'), parameters('objectect').vm.backupConfig.existingBackupVaultRG, 'nothing')]",
            "properties": {
                "mode": "Incremental",
                "template": {
                    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
                    "contentVersion": "1.0.0.0",
                    "resources": [
                        {
                            "name": "[concat(parameters('objectect').vm.backupConfig.existingBackupVaultName, '/','Azure', '/', variables('backupItemName'))]",
                            "apiVersion": "2017-07-01",
                            "type": "Microsoft.RecoveryServices/vaults/backupFabrics/backupProtectionIntent",
                            "properties": {
                                "friendlyName": "[concat(parameters('objectect').vm.computerName, 'BackupIntent')]",
                                "protectionIntentItemType": "AzureResourceItem",
                                "policyId": "[resourceId(parameters('objectect').vm.backupConfig.existingBackupVaultRG, 'Microsoft.RecoveryServices/vaults/backupPolicies', parameters('objectect').vm.backupConfig.existingBackupVaultName, parameters('objectect').vm.backupConfig.existingBackupPolicy)]",
                                "sourceResourceId": "[resourceId(resourceGroup().name, 'Microsoft.Compute/virtualMachines', concat(parameters('objectect').vm.computerName))]"
                            }
                        }
                    ]
                }
            },
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ]
        },
        {
            "condition": "[contains(parameters('objectect').vm,'antimalwareInfo')]",
            "name": "[concat('microsoft.antimalware-windows-20180924102643', parameters('objectect').vm.computerName)]",
            "apiVersion": "2015-01-01",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('windowsAntimalwareURI')]"
                },
                "parameters": {
                    "vmName": {
                        "value": "[parameters('objectect').vm.computerName]"
                    },
                    "location": {
                        "value": "[resourceGroup().location]"
                    },
                    "ExclusionsPaths": {
                        "value": "[parameters('objectect').vm.antimalwareInfo.exclusionPaths]"
                    },
                    "ExclusionsExtensions": {
                        "value": "[parameters('objectect').vm.antimalwareInfo.exclusionExtensions]"
                    },
                    "ExclusionsProcesses": {
                        "value": "[parameters('objectect').vm.antimalwareInfo.exclusionProcesses]"
                    },
                    "RealtimeProtectionEnabled": {
                        "value": "[parameters('objectect').vm.antimalwareInfo.realtimeProtectionEnabled]"
                    },
                    "ScheduledScanSettingsIsEnabled": {
                        "value": "[parameters('objectect').vm.antimalwareInfo.scheduledScanSettingsEnabled]"
                    },
                    "ScheduledScanSettingsScanType": {
                        "value": "[parameters('objectect').vm.antimalwareInfo.scheduledScanSettingsType]"
                    },
                    "ScheduledScanSettingsDay": {
                        "value": "[parameters('objectect').vm.antimalwareInfo.scheduledScanSettingsDay]"
                    },
                    "ScheduledScanSettingsTime": {
                        "value": "[parameters('objectect').vm.antimalwareInfo.scheduledScanSettingsTime]"
                    }
                }
            },
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ]
        },
        {
            "condition": "[contains(parameters('objectect'),'domainObject')]",
            "type": "Microsoft.Resources/deployments",
            "name": "[concat('DomainJoin-', parameters('objectect').vm.computerName)]",
            "apiVersion": "2018-02-01",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('domainJoinURI')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "adPassword": {
                        "reference": {
                            "keyVault": {
                                "id": "[concat('/subscriptions/', subscription().subscriptionId, '/resourceGroups/', parameters('objectect').vmKeyVault.keyVaultResourceGroupName, '/providers/Microsoft.KeyVault/vaults/', pspc.getValidKeyvaultName(pspc.getUniqueKeyvaultName(parameters('objectect').vmKeyVault.keyVaultName, pspc.getResourceGroupID(parameters('objectect').vmKeyVault.keyVaultResourceGroupName))))]"
                            },
                            "secretName": "[parameters('objectect').domainObject.domainUserSecretName]"
                        }
                    },
                    "objectect": {
                        "value": "[parameters('objectect')]"
                    }
                }
            }
        },
        {
            "condition": "[contains(parameters('objectect'),'omsAgentForLinux')]",
            "type": "Microsoft.Resources/deployments",
            "name": "[concat('OmsAgentForLinux-', parameters('objectect').vm.computerName)]",
            "apiVersion": "2018-02-01",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('omsAgentForLinuxURI')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "objectect": {
                        "value": "[parameters('objectect')]"
                    }
                }
            }
        },
        {
            "condition": "[contains(parameters('objectect'),'omsAgentForWindows')]",
            "type": "Microsoft.Resources/deployments",
            "name": "[concat('OmsAgentForWindows-', parameters('objectect').vm.computerName)]",
            "apiVersion": "2018-02-01",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('omsAgentForWindowsURI')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "objectect": {
                        "value": "[parameters('objectect')]"
                    }
                }
            }
        },
        {
            "condition": "[contains(parameters('objectect'),'AADLoginForLinux')]",
            "type": "Microsoft.Resources/deployments",
            "name": "[concat('Install-LinuxDomainJoin-Extension-', parameters('objectect').vm.computerName)]",
            "apiVersion": "2018-02-01",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('linuxDomainJoinURI')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "objectect": {
                        "value": "[parameters('objectect')]"
                    }
                }
            }
        },
        {
            "condition": "[contains(parameters('objectect').vm,'linuxScript')]",
            "name": "[concat(parameters('objectect').vm.computerName,'/LinuxCustomScript')]",
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "location": "[resourceGroup().location]",
            "apiVersion": "2015-06-15",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "publisher": "Microsoft.Azure.Extensions",
                "type": "CustomScript",
                "typeHandlerVersion": "2.0",
                "autoUpgradeMinorVersion": true,
                "settings": {},
                "protectedSettings": "[parameters('objectect').vm.linuxScript.protectedSettings]"
            }
        },
        {
            "condition": "[contains(parameters('objectect').vm,'encryptDisks')]",
            "type": "Microsoft.Resources/deployments",
            "name": "[concat('EncryptWindows-', parameters('objectect').vm.computerName)]",
            "apiVersion": "2018-02-01",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('encryptDisksWindowsURI')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "objectect": {
                        "value": "[parameters('objectect')]"
                    }
                }
            }
        },
        {
            "condition": "[contains(parameters('objectect').vm,'encryptDisksLinux')]",
            "type": "Microsoft.Resources/deployments",
            "name": "[concat('EncryptLinux-', parameters('objectect').vm.computerName)]",
            "apiVersion": "2018-02-01",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('encryptDisksLinuxURI')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "objectect": {
                        "value": "[parameters('objectect')]"
                    }
                }
            }
        },
        {
            "condition": "[contains(parameters('objectect').vm,'WebServerSettings')]",
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "name": "[concat(parameters('objectect').vm.computerName,'/', variables('iisExtensionName'))]",
            "apiVersion": "2015-05-01-preview",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "publisher": "Microsoft.Powershell",
                "type": "DSC",
                "typeHandlerVersion": "2.19",
                "autoUpgradeMinorVersion": true,
                "forceUpdateTag": "[variables('iisForceUpdateTag')]",
                "settings": {
                    "modulesUrl": "[variables('iisModuleUrl')]",
                    "configurationFunction": "[variables('iisConfigurationFunction')]",
                    "properties": {
                        "nodeName": "[parameters('objectect').vm.computerName]",
                        "webDeployPackage": "[parameters('objectect').vm.WebServerSettings.webDeployPackage]",
                        "certStoreName": "[parameters('objectect').vm.WebServerSettings.certificateStoreName]",
                        "certDomain": "[parameters('objectect').vm.WebServerSettings.certificateDomain]"
                    }
                },
                "protectedSettings": {}
            }
        },
        {
            "condition": "[contains(parameters('objectect').vm,'DSCSettings')]",
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "name": "[concat(parameters('objectect').vm.computerName,'/', 'DSCExtensionName')]",
            "apiVersion": "2019-03-01",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[concat('Deploy-', parameters('objectect').vm.computerName)]"
            ],
            "properties": {
                "publisher": "Microsoft.Powershell",
                "type": "DSC",
                "typeHandlerVersion": "2.77",
                "autoUpgradeMinorVersion": true,
                "settings": "[parameters('objectect').vm.DSCSettings]",
                "protectedSettings": {
                    "configurationUrlSasToken": "[parameters('containerSasToken')]"
                }
            }
        }
    ],
    "outputs": {
        "nic": {
            "type": "object",
            "value": "[reference(concat(parameters('objectect').vm.computerName, '-', parameters('objectect').networkInterfaces.name))]"
        },
        "WebServerSet": {
            "type": "bool",
            "value": "[contains(parameters('objectect').vm,'WebServerSettings')]"
        },
        "iisUrl": {
            "type": "string",
            "value": "[variables('iisModuleUrl')]"
        }
    }
}